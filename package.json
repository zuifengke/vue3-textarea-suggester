{
  "name": "vue-textarea-suggester",
  "type": "module",
  "description": "A Vuejs suggester",
  "version": "1.0.3",
  "author": {
    "name": "zuifengke",
    "email": "yehui1989@qq.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "rollup",
    "template",
    "suggester",
    "editor",
    "components"
  ],
  "license": "MIT",
  "main": "dist/vue-textarea-suggester.umd.js",
  "module": "dist/vue-textarea-suggester.esm.js",
  "unpkg": "dist/vue-textarea-suggester.min.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch",
    "build:es5": "babel dist -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuifengke/vue3-textarea-suggester.git"
  },
  "bugs": {
    "url": "https://github.com/zuifengke/vue3-textarea-suggester/issues"
  },
  "homepage": "https://github.com/zuifengke/vue3-textarea-suggester#readme",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@vue/compiler-sfc": "^3.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "rollup": "^4.34.8",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sass": "^1.15.2",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.2.0"
  },
  "browserslist": [
    "> 1%, not dead",
    "last 2 versions",
    "not ie <= 8"
  ]
}
